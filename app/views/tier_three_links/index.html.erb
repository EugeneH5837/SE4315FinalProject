<p id="notice"><%= notice %></p>

<h1>Tier Three Links</h1>

<table>
  <thead>
    <tr>
      <th>Links</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tier_three_links.each do |tier_three_link| %>
      <tr>
        <td><%= tier_three_link.links %></td>
        <td><%= link_to 'Show', tier_three_link %></td>
        <td><%= link_to 'Edit', edit_tier_three_link_path(tier_three_link) %></td>
        <td><%= link_to 'Destroy', tier_three_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tier Three Link', new_tier_three_link_path %>
