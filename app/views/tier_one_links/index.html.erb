<p id="notice"><%= notice %></p>

<h1>Global Links</h1>
<table>
  <thead>
    <tr>
      <th>Links</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @global_links.each do |global_links| %>
      <tr>
        <td><%= link_to global_links.link, global_links.link %></td>

        <% if false %>
        <td><%= link_to 'Show', global_links.link %></td>
        <td><%= link_to 'Edit', edit_tier_one_link_path(tier_one_link) %></td>
        <td><%= link_to 'Destroy', tier_one_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>

</table>


<h1>Tier One Admin Links</h1>

<table>
  <thead>
    <tr>
      <th>Links</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tier_one_links.each do |tier_one_link| %>
      <tr>
        <td><%= tier_one_link.links %></td>
        <% if false %>
        <td><%= link_to 'Show', tier_one_link %></td>
        <td><%= link_to 'Edit', edit_tier_one_link_path(tier_one_link) %></td>
        <td><%= link_to 'Destroy', tier_one_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>


<br>

<% if false %>
<%= link_to 'New Tier One Link', new_tier_one_link_path %>
<% end %>
